function _init()
	x = 64 --coords of top left of rect
	y = 64
end

function _update()
	if btn(0) then x -= 1 end
	if btn(1) then x += 1 end
	if btn(2) then y -= 1 end
	if btn(3) then y += 1 end
end

function _draw()
	cls()
	--rectfill(x, y, x+1, y+1, 8)
	pset(x, y, 8)
end
